    jp_dialog: "\u305D\u306E\u97F3\u306B\u30DE\u30EA\u30FC\u30B4\u30FC\u30EB\u30C9\u306F\u5370\u5237\u6A5F\u306E\u65B9\u3092\u898B\u305F\u3002\\n\u6A5F\u68B0\u306F\u305F\u3063\u305F\u4ECA\u4F9D\u983C\u3092\u7D42\u3048\u3066\u5E30\u3063\u3066\u304D\u305F\u30DE\u30EA\u30FC\u30B4\u30FC\u30EB\u30C9\u306A\u3069\u3082\u306E\u3068\u3082\u305B\u305A\u3001\u3044\u3064\u3082\u306E\u3088\u3046\u306B\u52DD\u624B\u306B\u65B0\u3057\u3044\u4F9D\u983C\u66F8\u3092\u629C\u304D\u51FA\u3057\u3066\u3044\u305F\u3002"
    ch0_dialog: "\u739B\u4E3D \u2219 \u91D1\u5FB7\u770B\u7740\u5370\u5237\u673A.\\n\u673A\u5668\u5E76\u4E0D\u7406\u4F1A\u521A\u521A\u5B8C\u6210\u59D4\u6258\u56DE\u6765\u7684\u91D1\u76CF\u82B1\uFF0C\u50CF\u5F80\u5E38\u4E00\u6837\uFF0C\u968F\u610F\u6253\u5370\u51FA\u65B0\u7684\u59D4\u6258\u4E66\u3002"
