    ch1_dialog: "\u9019\u6A23\u770B\u4F86,\u8207\u5176\u4ED6\u59D4\u8A17\u6C92\u6709\u4EC0\u9EBC\u5169\u6A23\u7684\u5E73\u51E1\u59D4\u8A17,\u4F46\u746A\u9E97\u91D1\u537B\u5947\u5999\u5730\u611F\u5230\u5FCC\u8AF1\u3002"
    ru_dialog: "\u0421\u043C\u043E\u0442\u0440\u044F \u043D\u0430 \u044D\u0442\u043E,
