    ch0_dialog: "\u739B\u4E3D \u2219 \u6208\u5C14\u5FB7\u53C8\u770B\u4E86\u4E00\u773C\u5851\u6599\u888B,\u611F\u5230\u6BDB\u9AA8\u609A\u7136.\\n\u770B\u5230\u5851\u6599\u888B\u91CC\u7684\u6837\u5B50\uFF0C\u4E07\u91CC\u91D1\u4E0D\u7531\u5F97\u53EB\u516C\u56ED\u90FD\u54CD\u4E86\u3002"
    ch1_dialog: "\u746A\u9E97 \u2219 \u6208\u723E\u5FB7\u53C8\u770B\u4E86\u4E00\u773C\u5851\u6599\u888B,\u611F\u5230\u6BDB\u9AA8\u609A\u7136.\\n\u770B\u5230\u5851\u6599\u888B\u88CF\u7684\u6A23\u5B50\uFF0C\u842C\u91CC\u91D1\u4E0D\u7531\u5F97\u53EB\u516C\u5712\u90FD\u97FF\u4E86\u3002"
    ru_dialog: "\u041C\u044D\u0440\u0438 \u0413\u043E\u043B\u0434 \u0435\u0449\u0451
      \u0440\u0430\u0437 \u043F\u043E\u0441\u043C\u043E\u0442\u0440\u0435\u043B\u0430
      \u043D\u0430 \u043F\u043E\u043B\u0438\u044D\u0442\u0438\u043B\u0435\u043D\u043E\u0432\u044B\u0439
      \u043F\u0430\u043A\u0435\u0442, \u043F\u043E\u0447\u0443\u0432\u0441\u0442\u0432\u043E\u0432\u0430\u0432
      \u0436\u0443\u0442\u043A\u043E\u0435 \u043E\u0449\u0443\u0449\u0435\u043D\u0438\u0435.\\n\u0423\u0432\u0438\u0434\u0435\u0432
      \u043E\u0431\u0440\u0430\u0437 \u0432 \u043F\u043E\u043B\u0438\u044D\u0442\u0438\u043B\u0435\u043D\u043E\u0432\u043E\u043C
      \u043F\u0430\u043A\u0435\u0442\u0435, \u041C\u044D\u0440\u0438 \u0413\u043E\u043B\u0434
      \u043F\u0440\u0435\u043A\u0440\u0430\u0442\u0438\u043B\u0430 \u043A\u0440\u0438\u0447\u0430\u0442\u044C,
      \u0447\u0442\u043E\u0431\u044B \u0432\u0435\u0441\u044C \u043F\u0430\u0440\u043A
