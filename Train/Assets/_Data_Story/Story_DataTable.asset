    jp_dialog: "\u7A81\u7136\u73FE\u308C\u305F\u5C0F\u3055\u306A\u4EBA\u98DF\u3044\u5909\u7570\u52D5\u7269\u3060\u3068\u601D\u3063\u305F\u304C\u3001\u3088\u304F\u898B\u308B\u3068\u305F\u3060\u79C1\u306E\u679C\u7269\u3092\u71B1\u5FC3\u306B\u98DF\u3079\u3066\u3044\u305F\u3060\u3051\u3060\u3063\u305F\u3002"
    ch0_dialog: "\u6211\u4EE5\u4E3A\u662F\u7A81\u7136\u51FA\u73B0\u7684\u5C0F\u98DF\u4EBA\u53D8\u5F02\u52A8\u7269\uFF0C\u4ED4\u7EC6\u4E00\u770B\uFF0C\u53EA\u662F\u8BA4\u771F\u5403\u7740\u81EA\u5DF1\u7684\u6C34\u679C\u3002"
    ch1_dialog: "\u6211\u4EE5\u7232\u662F\u7A81\u7136\u51FA\u73FE\u7684\u5C0F\u98DF\u4EBA\u8B8A\u7570\u52D5\u7269\uFF0C\u4ED4\u7D30\u4E00\u770B\uFF0C\u53EA\u662F\u8A8D\u771F\u5403\u7740\u81EA\u5DF1\u7684\u6C34\u679C\u3002"
    ru_dialog: "\u042F \u0434\u0443\u043C\u0430\u043B, \u0447\u0442\u043E \u044D\u0442\u043E
      \u043C\u0430\u043B\u0435\u043D\u044C\u043A\u0438\u0439 \u0447\u0435\u043B\u043E\u0432\u0435\u043A\u043E\u043F\u043E\u0434\u043E\u0431\u043D\u044B\u0439
      \u0437\u0432\u0435\u0440\u044C, \u043A\u043E\u0442\u043E\u0440\u044B\u0439
      \u0432\u043D\u0435\u0437\u0430\u043F\u043D\u043E \u043F\u043E\u044F\u0432\u0438\u043B\u0441\u044F,
      \u043D\u043E \u0435\u0441\u043B\u0438 \u043F\u0440\u0438\u0441\u043C\u043E\u0442\u0440\u0435\u0442\u044C\u0441\u044F,
      \u0442\u043E \u043F\u0440\u043E\u0441\u0442\u043E \u0443\u0441\u0435\u0440\u0434\u043D\u043E
      \u0435\u043B \u043C\u043E\u0438 \u0444\u0440\u0443\u043A\u0442\u044B."
