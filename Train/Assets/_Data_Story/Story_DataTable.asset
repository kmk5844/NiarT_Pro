    ch1_dialog: "\u99AC\u88CF\u6208\u723E\u5FB7\u7528\u8173\u8DBE\u628A\u6C99\u5B50\u63A8\u5230\u4E86\u5751\u9644\u8FD1\u3002\u7FFB\u8B6F:\u7136\u5F8C\u4ED6\u62FF\u51FA\u888B\u5B50\u88CF\u6700\u6C92\u7528\u7684\u6771\u897F\uFF0C\u5C0F\u5FC3\u7FFC\u7FFC\u5730\u653E\u5728\u90A3\u88CF\u3002"
    ru_dialog: "\u041C\u0430\u0440\u0438\u0433\u043E\u043B\u044C\u0434 \u0441 \u043F\u0430\u043B\u044C\u0446\u0430\u043C\u0438
