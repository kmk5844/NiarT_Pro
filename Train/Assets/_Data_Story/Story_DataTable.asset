    ch0_dialog: "\u770B\u5230\u5546\u5E97\u5927\u53D4\u56F4\u89C2\u7684\u6211\uFF0C\u6211\u7ED9\u4E86\u4ED6\u4E00\u628A\u6A31\u6843\u548C\u8349\u8393\u3002\\n\u8FD9\u6837\u80FD\u505A\u751F\u610F\u5417\uFF1F
      \u672C\u6765\u662F\u60F3\u5403\u7684\u91CF,\u4F46\u5403\u4E86\u4E00\u53E3\u540E\u53D1\u73B0,\u7ED9\u8FD9\u4E2A\u6C34\u679C\u7684\u5927\u53D4\u81EA\u4FE1\u6EE1\u6EE1\u7684\u8868\u60C5\u5E76\u6CA1\u6709\u767D\u8D39\u3002"
    ch1_dialog: "\u770B\u5230\u5546\u5E97\u5927\u53D4\u570D\u89C0\u7684\u6211\uFF0C\u6211\u7D66\u4E86\u4ED6\u4E00\u628A\u6AFB\u6843\u548C\u8349\u8393\u3002\\n\u9019\u6A23\u80FD\u505A\u751F\u610F\u55CE\uFF1F
      \u672C\u4F86\u662F\u60F3\u5403\u7684\u91CF,\u4F46\u5403\u4E86\u4E00\u53E3\u5F8C\u767C\u73FE,\u7D66\u9019\u500B\u6C34\u679C\u7684\u5927\u53D4\u81EA\u4FE1\u6EFF\u6EFF\u7684\u8868\u60C5\u4E26\u6C92\u6709\u767D\u8CBB\u3002"
    ru_dialog: "\u041A\u043E\u0433\u0434\u0430 \u0434\u044F\u0434\u044F \u043C\u0430\u0433\u0430\u0437\u0438\u043D\u0430
