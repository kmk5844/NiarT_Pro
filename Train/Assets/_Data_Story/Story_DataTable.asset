    ch1_dialog: "\u4E00\u4F38\u624B\u6574\u7406\u6587\u4EF6\u7684\u77AC\u9593\uFF0C\u516C\u53F8\u7684\u820A\u5370\u5237\u6A5F\u50CF\u5F80\u5E38\u4E00\u6A23\u767C\u51FA\u5947\u602A\u7684\u8072\u97F3\u3002\u4F34\u96A8\u7740\u54AF\u5654\u54AF\u5654\u7684\u5927\u566A\u97F3,\u6B63\u5728\u6253\u5370\u5BEB\u6709\u59D4\u8A17\u5167\u5BB9\u7684\u7D19\u3002"
    ru_dialog: "\u0412 \u0442\u043E\u0442 \u043C\u043E\u043C\u0435\u043D\u0442, \u043A\u043E\u0433\u0434\u0430
