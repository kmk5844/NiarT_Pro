    ch0_dialog: "\u9A6C\u91CC\u6208\u5C14\u5FB7\u6CAE\u4E27\u5730\u7ACB\u523B\u6253\u5F00\u8F66\u95E8\uFF0C\u8D70\u51FA\u4E86\u706B\u8F66\u3002\u7FFB\u8BD1:\u9A6C\u91CC\u6208\u5C14\u5FB7\u68C0\u67E5\u4E86\u706B\u8F66\u51E0\u5206\u949F\u540E\uFF0C\u5F53\u573A\u6251\u5012\u5728\u5730\u3002"
    ch1_dialog: "\u99AC\u88CF\u6208\u723E\u5FB7\u6CAE\u55AA\u5730\u7ACB\u523B\u6253\u958B\u8ECA\u9580\uFF0C\u8D70\u51FA\u4E86\u706B\u8ECA\u3002\u7FFB\u8B6F:\u99AC\u88CF\u6208\u723E\u5FB7\u6AA2\u67E5\u4E86\u706B\u8ECA\u5E7E\u5206\u9418\u5F8C\uFF0C\u7576\u5834\u64B2\u5012\u5728\u5730\u3002"
    ru_dialog: "\u0420\u0430\u0437\u043E\u0447\u0430\u0440\u043E\u0432\u0430\u0432\u0448\u0438\u0441\u044C,
      \u041C\u0430\u0440\u0438\u0433\u043E\u043B\u044C\u0434 \u043D\u0435\u043C\u0435\u0434\u043B\u0435\u043D\u043D\u043E
