    ch1_dialog: "\u96D6\u7136\u746A\u9E97\u91D1\u56E0\u7232\u525B\u7E94\u767C\u751F\u7684\u4E8B\u60C5\u800C\u982D\u75BC\u4E0D\u5DF2,\u4F46\u64D4\u5FC3\u8E29\u5230\u788E\u73BB\u7483\u788E\u7247\u7684\u746A\u9E97\u91D1\u62FF\u51FA\u623F\u9593\u88CF\u7684\u5C0F\u6E05\u6383\u5DE5\u5177,\u5C07\u788E\u789F\u5B50\u88DD\u5728\u7C38\u7B95\u88CF\u3002"
    ru_dialog: "\u0423 \u041C\u044D\u0440\u0438\u0433\u043E\u043B\u0434\u0430 \u0431\u043E\u043B\u0438\u0442
