    ch1_dialog: "\u62FF\u7740\u66F8\u82E6\u60F1\u4E86\u4E00\u6703\u5152\u7684\u746A\u9E97\xB7\u6208\u723E\u5FB7\u7B11\u7740\u74B0\u9867\u56DB\u5468\uFF0C\u78BA\u8A8D\u6C92\u6709\u4EBA\u5F8C\uFF0C\u5750\u5728\u820A\u6905\u5B50\u4E0A\u958B\u59CB\u7FFB\u958B\u66F8\u3002"
