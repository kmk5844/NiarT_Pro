    ch0_dialog: "\u8FD9\u4E9B\u52AB\u63A0\u8005\u6709\u70B9\u4E0D\u540C\u3002 \u8001\u98DE\u884C\u5458\u4E0A\u7684\u90A3\u4E9B\u4EBA\u4E00\u8DEF\u75BE\u9A70\u5230\u7EC8\u70B9\u5E76\u53D1\u52A8\u4E86\u653B\u51FB\uFF0C\u6216\u8BB8\u4ED6\u4EEC\u8BA4\u4E3A\u8FD9\u662F\u4E00\u4E2A\u7528\u5168\u526F\u96C7\u4F63\u5175\u5EFA\u9020\u5E76\u7EF4\u62A4\u516C\u53F8\u5217\u8F66\u7684\u597D\u673A\u4F1A\u3002"
    ch1_dialog: "\u9019\u4E9B\u52AB\u63A0\u8005\u6709\u9EDE\u4E0D\u540C\u3002 \u8001\u98DB\u884C\u54E1\u4E0A\u7684\u90A3\u4E9B\u4EBA\u4E00\u8DEF\u75BE\u99B3\u5230\u7D42\u9EDE\u4E26\u767C\u52D5\u4E86\u653B\u64CA\uFF0C\u6216\u8A31\u4ED6\u5011\u8A8D\u7232\u9019\u662F\u4E00\u500B\u7528\u5168\u526F\u50F1\u50AD\u5175\u5EFA\u9020\u4E26\u7DAD\u8B77\u516C\u53F8\u5217\u8ECA\u7684\u597D\u6A5F\u6703\u3002"
