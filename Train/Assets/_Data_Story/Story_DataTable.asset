    ch1_dialog: "\u7DCA\u5F35\u4E0D\u5B89\u7684\u52AB\u63A0\u8005\u5011\u4E92\u76F8\u8A0E\u8AD6\u7740\uFF0C\u5411\u7A97\u5916\u548C\u6A5F\u8EAB\u5167\u90E8\u5F35\u671B\u3002\u7FFB\u8B6F:\u99D5\u99DB\u8259\u53CA\u5176\u5468\u570D\u7684\u5EA7\u4F4D\u4E0A\u6709\u4E00\u4E9B\u5C0F\u6771\u897F\uFF0C\u4F46\u8CA8\u8259\u88CF\u537B\u5145\u6EFF\u4E86\u5927\u5927\u5C0F\u5C0F\u7684\u8208\u8DA3\u3002"
    ru_dialog: "\u041D\u0435\u0440\u0432\u043D\u044B\u0435 \u0438 \u0431\u0435\u0441\u043F\u043E\u043A\u043E\u0439\u043D\u044B\u0435,
