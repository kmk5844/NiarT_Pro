    ch0_dialog: "\u90A3\u65F6\u7684\u732A\u867D\u7136\u53EB\u4E86\u4E00\u58F0\uFF0C\u4F46\u89C1\u5230\u6211\u4E5F\u4E0D\u611F\u5230\u60CA\u8BB6\u3002\\n\u53EA\u662F\u9759\u9759\u5730\u652F\u652F\u543E\u543E\u5730\u5403\u4E1C\u897F\u3002"
    ch1_dialog: "\u90A3\u6642\u7684\u8C6C\u96D6\u7136\u53EB\u4E86\u4E00\u8072\uFF0C\u4F46\u898B\u5230\u6211\u4E5F\u4E0D\u611F\u5230\u9A5A\u8A1D\u3002\\n\u53EA\u662F\u975C\u975C\u5730\u652F\u652F\u543E\u543E\u5730\u5403\u6771\u897F\u3002"
    ru_dialog: "\u0421\u0432\u0438\u043D\u044C\u044F \u0432 \u0442\u043E \u0432\u0440\u0435\u043C\u044F
      \u043D\u0435 \u0443\u0434\u0438\u0432\u0438\u043B\u0430\u0441\u044C, \u0443\u0432\u0438\u0434\u0435\u0432
      \u043C\u0435\u043D\u044F, \u043D\u0435\u0441\u043C\u043E\u0442\u0440\u044F
